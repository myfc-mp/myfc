<!--本页是普通用户登陆页面-->
<!-- 引入房屋信息显示模板 -->
<import src="normal-tpl/normal-tpl.wxml" />  
<view class="normal-box">
  <text class="normal-title">普通首页</text>
    <image class="normal-logo" src="/image/new-maya-web.png"></image>
  <text class="normal-newhouse">最新房源</text>
  <scroll-view scroll-y style="width: 100%">
    <view class="tpl-line-box" wx:for="{{proList}}" wx:for-item="item" wx:key="title">
        <!-- 引入normal-item房屋信息显示模板 -->
        <template is = "normal-item" data="{{item}}"/>
    </view>
  </scroll-view>
</view> 