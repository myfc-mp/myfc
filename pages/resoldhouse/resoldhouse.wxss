
.swiperBox{
  width: 100%;
  height:450rpx;
}
.swiper{
  width: 100%;
  height:100%;
}
.swiperBox image{
  width: 100%; 
  height:100%;
}

.title{
  display: flex;
  height: 120rpx;
  align-items: center;
  border-bottom: 1rpx solid #f5f5f5; 
}

.titleName{
  width: 580rpx;
  height: 100%;
  font-size: 35rpx;
  margin-top: 30rpx;
  margin-left: 30rpx;
}

.forward{
  height: 110rpx;
  width: 110rpx;   
}

.forward button{
  height: 110rpx;
  width: 110rpx;
  padding: 0 0;
  border:none; 
}
 .forward image{
   width: 100%;
   height: 100%;
 }

 .overview{
   display: flex;
   height:110rpx;
   text-align: center;
   border-bottom: 1rpx solid #f5f5f5; 
   padding-top: 30rpx;
 }

 .price{
   flex:1;
   
 }

 .type{
   flex: 1;
 }

 .area{
   flex: 1;
 }

.overview-value{
  font-size: 30rpx;
  color: RGB(192,0,0);
  font-weight: bold;
}

.overview-describ{
  font-size: 25rpx;
  color: RGB(92,88,88);
}

.division{
  height: 80rpx;
  border-left: 1rpx solid #f5f5f5; 
}

.info{
  display: flex;
 
  border-bottom: 1rpx solid #f5f5f5; 
  padding: 30rpx 30rpx;
  font-size: 28rpx;
  line-height: 50rpx;
  color: RGB(92,88,88); 
}

.info-left{
  flex: 2;
}

.info-right{
  flex: 1;
}

.agency{
  display: flex;
  height:160rpx;
  padding: 0 30rpx;
  border-bottom: 1rpx solid #f5f5f5; 
  color: RGB(92,88,88);
  font-size: 30rpx;
  align-items: center;
}

.avatar{
  height:120rpx;
  width: 120rpx;
  margin-right: 30rpx;
}

.avatar image{
  height: 100%;
  width: 100%;
}

.agency-info{
  height:120rpx;
  width: 450rpx;
  line-height: 60rpx;
}

.agency-call{
  height:120rpx;
  width: 120rpx;
  line-height: 60rpx;

}

.agency-call image{
  height:120rpx;
  width: 120rpx;

}

.detail-title{
  font-size: 30rpx;
  border-top: 20rpx solid #f5f5f5;
  border-bottom: 1rpx solid #f5f5f5;
  padding: 10rpx 30rpx;
}

.detail-label{
  padding: 30rpx;
}

.tpl-label{
  font-size:25rpx;
  border-radius: 5rpx;
  margin-right: 10rpx;
  margin-bottom: 10rpx;
  text-align: center;
  padding: 0 6rpx;
}

.competance{
  padding: 0 30rpx 30rpx 30rpx;
  font-size:25rpx;
}

.around{
  padding: 30rpx;
  font-size:25rpx;
}

.competance-title{
  display: flex;
  font-size:27rpx;
  color: green;
  margin-bottom: 20rpx;
  align-items: center;
}

.around-title{
  display: flex;
  font-size:27rpx;
  color: green;
  margin-bottom: 20rpx;
  align-items: center;
}

.iconfont{
  font-family:"iconfont" !important;
  font-size:18px;
  font-style:normal;
}

.icon-competence:before { content: "\e092"; }
.icon-around:before { content: "\e093"; }
.icon-location:before { content: "\e094"; }

.competance-title-text{
  margin-left: 10rpx;
}

.map{
  margin: 0 30rpx 30rpx 30rpx;
}