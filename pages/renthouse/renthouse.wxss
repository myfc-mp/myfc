
.swiperBox{
  width: 100%;
  height:450rpx;
}
.swiper{
  width: 100%;
  height:100%;
}
.swiperBox image{
  width: 100%; 
  height:100%;
}

.title{
  display: flex;
  height: 120rpx;
  align-items: center;
  border-bottom: 1rpx solid #f5f5f5; 
}

.titleName{
  width: 580rpx;
  height: 100%;
  font-size: 35rpx;
  margin-top: 30rpx;
  margin-left: 30rpx;
}

.forward{
  height: 110rpx;
  width: 110rpx;   
}

.forward button{
  height: 110rpx;
  width: 110rpx;
  padding: 0 0;
  border:none; 
}
 .forward image{
   width: 100%;
   height: 100%;
 }

 .overview{
   display: flex;
   height:110rpx;
   text-align: center;
   border-bottom: 1rpx solid #f5f5f5; 
   padding-top: 30rpx;
 }

 .price{
   flex:1;
   
 }

 .type{
   flex: 1;
 }

 .area{
   flex: 1;
 }

.overview-value{
  font-size: 30rpx;
  color: RGB(192,0,0);
  font-weight: bold;
}

.overview-describ{
  font-size: 25rpx;
  color: RGB(92,88,88);
}

.division{
  height: 80rpx;
  border-left: 1rpx solid #f5f5f5; 
}