@import "../tpls/simple-tpl/simple-tpl.wxss"; 
.frame {
  display: flex;
  flex-direction: column;   
  background-color: #fff; 
  margin-top:80rpx;   
}

.selectZone{
  position:fixed;
  top:0%;
  width: 100%;
  height: 80rpx;
  color: RGB(92,88,88);
  font-size: 30rpx;
  background-color:white;
  padding: 20rpx 0;
}
.innerZone{
  display: flex;
}

.selectTitle{
  display: flex;
  flex:1;
  align-items: center;
  padding: 0 30rpx;
}
.arrow{
  height: 10rpx;
  width: 0rpx;
  border-top: 10rpx solid RGB(92,88,88);
  border-left: 10rpx solid white;
  border-right: 10rpx solid white;
  margin-left: 10rpx;
}

.selectDetaile{
  color: RGB(92,88,88);
  font-size: 30rpx;
  padding: 0 30rpx;
}

.selectDetaile view{
  border-bottom: 1rpx solid #f5f5f5; 
  height: 60rpx;
  padding-top:30rpx;
}

.header{
  display: flex;
  flex-direction: row;
  text-align: left;
  color: #888;
  font-size: 25rpx; 

}

.header view{
  flex:1;
  padding:0 30rpx;
}