@import "../tpls/simple-tpl/simple-tpl.wxss"; 
.selectZone{
  display: flex;
  color: RGB(92,88,88);
  font-size: 30rpx;
}

.selectTitle{
  display: flex;
  flex:1;
  align-items: center;
  padding: 30rpx;
}
.arrow{
  height: 10rpx;
  width: 0rpx;
  border-top: 10rpx solid RGB(92,88,88);
  border-left: 10rpx solid white;
  border-right: 10rpx solid white;
  margin-left: 10rpx;
}

.selectDetaile{
  color: RGB(92,88,88);
  font-size: 30rpx;
  padding: 0 30rpx;
}

.selectDetaile view{
  border-bottom: 1rpx solid #f5f5f5; 
  height: 60rpx;
  padding-top:30rpx;
}